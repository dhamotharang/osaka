{
  "AllowedHosts": "*",
  "Logging": {
    "LogLevel": {
      "Default": "Warning",
      "HappyTravel": "Information",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "Vault": {
    "Endpoint": "HTDC_VAULT_ENDPOINT",
    "Engine": "secrets",
    "Role": "osaka",
    "Token": "HTDC_VAULT_TOKEN"
  },
  "Authority": {
    "Options": "osaka/authority"
  },
  "Jaeger": {
    "AgentHost": "JAEGER_AGENT_HOST",
    "AgentPort": "JAEGER_AGENT_PORT"
  },
  "Elasticsearch": {
    "ClientSettings": "osaka/elasticsearch/client-settings",
    "Indexes": {
      "En" : "osaka/elasticsearch/indexes/en"
    },
    "Mapper": "osaka/mapper"
  },
  "Redis": {
    "Endpoint": "HTDC_REDIS_HOST"
  },
  "PredictionsUpdate": {
    "Redis": "osaka/predictions-update/redis",
    "RedisStreams": "osaka/predictions-update/redis/streams"
  },
  "Telemetry": {
    "IsEnabled": false
  }
}
