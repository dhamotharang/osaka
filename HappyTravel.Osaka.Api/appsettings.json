{
  "AllowedHosts": "*",
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    },
    "Sentry": {
      "Endpoint": "HTDC_EDO_SENTRY_ENDPOINT"
    }
  },
  "Vault": {
    "Endpoint": "HTDC_VAULT_ENDPOINT",
    "Engine": "secrets",
    "Role": "osaka",
    "Token": "HTDC_VAULT_TOKEN"
  },
  "Authority": {
    "Options": "osaka/authority"
  },
  "Jaeger": {
    "AgentHost": "JAEGER_AGENT_HOST",
    "AgentPort": "JAEGER_AGENT_PORT"
  },
  "Elasticsearch": {
    "ClientSettings": "osaka/elasticsearch/client-settings",
    "Indexes": "osaka/elasticsearch/indexes",
    "Mapper": "osaka/mapper"
  },
  "Redis": {
    "Endpoint": "HTDC_REDIS_HOST"
  },
  "PredictionsUpdate": {
    "Role": "predictions-update",
    "Redis": "predictions-update/redis"
  }
}
